<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="startingS" prefHeight="800.0"
            prefWidth="1238.0" style="-fx-background-color: #6BDCF3#6BDCF3;"
            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="gui.StartingScreenController">
    <children>
        <Button fx:id="createGame" layoutX="348.0" layoutY="451.0" mnemonicParsing="false"
                onMouseClicked="#handleCreateGame" prefHeight="70.0" prefWidth="180.0" text="Create Game"
                textAlignment="CENTER" wrapText="true">
            <font>
                <Font name="Franklin Gothic Medium" size="23.0"/>
            </font>
        </Button>
        <Label alignment="CENTER" layoutX="374.0" layoutY="99.0"
               style="-fx-background-color: transparent;" text="RUMMYKUB" textAlignment="CENTER"
               AnchorPane.leftAnchor="374.0" AnchorPane.rightAnchor="375.0">
            <font>
                <Font name="Franklin Gothic Medium" size="94.0"/>
            </font>
        </Label>
        <Label layoutX="296.0" layoutY="93.0" rotate="-25.0" text="Welcome to">
            <font>
                <Font name="Franklin Gothic Medium" size="27.0"/>
            </font>
        </Label>
        <Button fx:id="joinGame" layoutX="675.0" layoutY="447.0" mnemonicParsing="false"
                onMouseClicked="#handleJoinGame" prefHeight="70.0" prefWidth="180.0" text="Join Game">
            <font>
                <Font name="Franklin Gothic Medium" size="23.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
